{
  "parser": "babel-eslint",
  "extends": "standard",
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "quotes": [
      "warn",
      "backtick"
    ]
  }
}
